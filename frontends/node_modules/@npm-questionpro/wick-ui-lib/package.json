    "@npm-questionpro/wick-ui-icon": ">= 6.1.0",
    "react": ">= 18 < 20",
    "react-dom": ">= 18 < 20"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier -w",
      "eslint --fix"
    ]
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "lint": "eslint ./src --fix",
    "lint:ci": "tsc --noEmit && eslint --max-warnings 0 ./src",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "preview": "vite preview",
    "dev": "storybook dev -p 6006",
    "build-storybook": "storybook build && cp -r cloudflarePages/* storybook-static/",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:ci": "vitest --coverage",
    "release:minor": "pnpm version minor",
    "release:patch": "pnpm version patch",
    "release:major": "pnpm version major"
  }
